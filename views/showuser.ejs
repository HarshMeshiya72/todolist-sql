<!DOCTYPE html>
<html lang="en">

<head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>Document</title>
</head>

<body>
      <table border="2">
            <% result.forEach(function(data){%>
                  <tr>
                        <td>
                              <%= data.name%>
                        </td>
                        <td>
                              <%= data.email%>
                        </td>
                        <td>
                              <%= data.password%>
                        </td>
                        <td>
                              <a href="/admin/showuser/delete/<%= data.id%>">
                                    Delete
                              </a>
                        </td>
                        <td>
                              <a href="/admin/showuser/update/<%= data.id%>">
                                    Update
                              </a>
                        </td>
                  </tr>
                  <%});%>

      </table>
</body>

</html>