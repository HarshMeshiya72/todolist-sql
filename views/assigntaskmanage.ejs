<!DOCTYPE html>
<html lang="en">

<head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>Document</title>
</head>

<body>
      <nav>
            <ul>
                  <li>
                        <a href="/assigntask">Assign Task</a>
                  </li>
                  <li>
                        <a href="/assigntask/manage">Assign Task Manage</a>
                  </li>
                  <li>
                        <a href="/task/complete">Complete Task</a>
                  </li>
                  <li>
                        <a href="/task/decline">Decline Task</a>
                  </li>
                  <li>
                        <a href="/task/start">Start Task</a>
                  </li>
                  <li>
                        <a href="/task/running">Running Task</a>
                  </li>
                  <li>
                        <a href="/task/pending">Pending Task</a>
                  </li>
                  <li>
                        <a href="/assigntask/logout">Logout</a>
                  </li>
            </ul>
      </nav>
      <table border="">
            <th>
                  Name
            </th>
            <th>
                  Task
            </th>
            <th>
                  Starting Date
            </th>
            <th>
                  Ending Date
            </th>
            <th>
                  Status
            </th>
            <th>
                  Manage Task Status
            </th>
            <%result.forEach(function(data){%>
                  <tr>
                        <td>
                              <%= data.name%>
                        </td>
                        <td>
                              <%= data.task%>
                        </td>
                        <td>
                              <%= data.sdate%>
                        </td>
                        <td>
                              <%= data.edate%>
                        </td>
                        <td>
                              <%= data.status%>
                        </td>
                        <td>
                              <a href="/assigntask/manage/<%= data.id %>">Manage Task Status</a>
                        </td>
                  </tr>
                  <%})%>
      </table>
</body>

</html>